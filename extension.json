{
	"name": "SimplePlantUML",
	"version": "0.4",
	"author": [
		"jmnote"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SimplePlantUML",
	"description": "render UML image between <code><nowiki><uml></nowiki></code> and <code><nowiki></uml></nowiki></code>",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"SimplePlantUML": "SimplePlantUML_body.php"
	},
	"callback": "SimplePlantUML::onRegistration",
	"ResourceModules": {
		"ext.SimplePlantUML": {
			"scripts": [
				"ext.SimplePlantUML.js"
			],
			"styles": [
				"ext.SimplePlantUML.css"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "SimplePlantUML/modules"
	},
	"manifest_version": 2
}